        android:supportsRtl="true"
        android:theme="@style/Theme.PracticaEnGrupo1Android">
        <activity android:name=".Camara"
                  android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"></activity>
        <activity android:name=".InspeccionarProducto"
                  android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"></activity>
        <activity android:name=".MainActivity"
                  android:label="1ª práctica en grupo"
                  android:theme="@style/Theme.AppCompat.DayNight">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>